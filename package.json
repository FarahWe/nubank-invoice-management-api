{
  "name": "rubik",
  "version": "1.0.0",
  "description": "Docs, Spaces, Alerts Management",
  "main": "src/index.js",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "build": "swc src --ignore **/*.test.js",
    "dev": "dotenv -e .env.development nodemon - exec 'ts-node' src/index.ts",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:cov": "dotenv -e .env.test -- vitest run --coverage",
    "test:debug": "dotenv -e .env.test -- vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "dotenv -e .env.test -- vitest run --config ./vitest.config.e2e.ts --no-threads",
    "prisma:dev:studio": "dotenv -e .env.development prisma studio",
    "prisma:dev:migrate": "dotenv -e .env.development prisma migrate dev",
    "prisma:dev:seed": "dotenv -e .env.development ts-node prisma/seed.ts",
    "prisma:test:seed": "dotenv -e .env.test ts-node prisma/seed.ts",
    "prisma:test:migrate": "dotenv -e .env.test prisma migrate dev",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "-": "^0.0.1",
    "@aws-sdk/client-s3": "^3.421.0",
    "@aws-sdk/client-ses": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.421.0",
    "@bull-board/express": "^5.14.1",
    "@notionhq/client": "^2.2.14",
    "@prisma/client": "^5.4.1",
    "D": "^1.0.0",
    "add": "^2.0.6",
    "bcryptjs": "^2.4.3",
    "bullmq": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status": "^1.7.0",
    "joi": "^17.10.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.0",
    "morgan": "^1.10.0",
    "nubank-api": "^3.1.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.11.3",
    "pm2": "^5.3.0",
    "swagger-ui-express": "^5.0.0",
    "validator": "^13.11.0",
    "winston": "^3.10.0",
    "yarn": "^1.22.21"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.1.0",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.101",
    "@types/bcryptjs": "^2.4.4",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-rate-limit": "^6.0.0",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.6.4",
    "@types/passport": "^1.0.12",
    "@types/supertest": "^2.0.13",
    "@types/swagger-ui-express": "^4.1.6",
    "@vitest/coverage-v8": "^0.34.3",
    "coveralls": "^3.0.7",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.4.0",
    "lint-staged": "^14.0.1",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.1",
    "supertest": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unplugin-swc": "^1.4.2",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.3"
  }
}
